START > statements;
modifiers > publicity modifiersOne | modifiersOne;
publicity > ID(public) | ID(private);
modifiersOne > scope modifiersTwo | modifiersTwo;
scope > ID(scope) | ID(static) | instance;
instance > ID(inst) | ID(instance);
modifiersTwo > mutability concurrency | concurrency;
mutability > mutable | immutable;
mutable > ID(var) | ID(variable);
immutable > ID(val) | ID(value);
concurrency >  synchronous | asynchronous ;
synchronous > ID(sync) | ID(synchronized);
asynchronous > ID(async) | ID(asynchronous);
format > ID() formatArgument;
formatArgument  > SYM(<) arguments SYM(>) | EPSILON();
formatArguments > formatArgumentGroup SYM(;) formatArguments | formatArgumentGroup;
formatArgumentGroup   > format SYM(,) formatArgumentGroup | format;
block > SYM({) statements SYM(});
statements > statement statements | EPSILON();
statement > highLevel | executable | controlFlow;
highLevel > import_stmt | constructor_stmt;
import_stmt > ID("import") import SYM(;);
import > ID() | STR();
constructor_stmt > modifiers constructor_decl | constructor_decl;
constructor_decl > ID("constructor") lamda;
executable > function_stmt | variable_stmt;
declaration > modifiers format;
variable_stmt > declaration assignment | assignment;
assignment > ID() SYM(=) expression;
function_stmt > declaration function_decl | function_call;
function_decl > ID() declarative_lamda;
declarative_lamda > SYM(() declarative_arguments SYM()) block;
declarative_arguments > declarative_argument SYM(,) declarative_arguments | declarative_argument;
declarative_argument > format ID() | format SYM(.) SYM(.) SYM(.) ID();
function_call > ID() SYM(() arguments SYM());
lamda > SYM(() arguments SYM()) block;
arguments > argumentGroup SYM(;) arguments | argumentGroup;
argumentGroup > expression SYM(,) argumentGroup | expression;
controlFlow > conditional_stmt | loop_stmt;
conditional_stmt > ID(if) SYM(() expression SYM()) block;
loop_stmt > ID(for) SYM(() variable_stmt SYM(;) expression SYM(;) expression SYM()) block;
expression > logical_or_expression| logical_or_expression SYM(?) expression SYM(:) expression;
logical_or_expression > logical_or_expression SYM(|) NUM() STR() logical_and_expression	| logical_and_expression;
logical_and_expression > logical_and_expression SYM(&) SYM(&) bitwise_or_expression	| bitwise_or_expression;
bitwise_or_expression > bitwise_or_expression SYM(|) bitwise_xor_expression | bitwise_xor_expression;
bitwise_xor_expression > bitwise_xor_expression SYM(*) SYM(|) bitwise_and_expression | bitwise_and_expression;
bitwise_and_expression > bitwise_and_expression SYM(&) equality_expression | equality_expression;
equality_expression	> equality_expression SYM(=) NUM() relational_expression | equality_expression SYM(!) SYM(=) relational_expression | relational_expression;
relational_expression > relational_expression SYM(<) shift_expression | relational_expression SYM(<) SYM(=) shift_expression | relational_expression SYM(>) shift_expression | relational_expression SYM(>) SYM(=) shift_expression | shift_expression;
shift_expression > shift_expression SYM(<) STR()ID(for) additive_expression | shift_expression SYM(>) SYM(>) additive_expression | shift_expression SYM(>) SYM(>) additive_expression | additive_expression;
additive_expression > additive_expression SYM(+) multiplicative_expression | additive_expression SYM(-) multiplicative_expression | multiplicative_expression;
multiplicative_expression > multiplicative_expression SYM(*) exponentative_expression | multiplicative_expression SYM(/) exponentative_expression | multiplicative_expression SYM(%) exponentative_expression | exponentative_expression;
exponentative_expression > exponentative_expression SYM(^) unary_expression | unary_expression;
unary_expression > SYM(+) unary_expression | SYM(-) unary_expression | SYM(+) SYM(+) unary_expression | SYM(-) SYM(-) unary_expression | SYM(%) unary_expression | SYM(!) unary_expression | postfix_expression;
postfix_expression > postfix_expression SYM(+) SYM(+) | postfix_expression SYM(-) SYM(-) | primary;
primary > STR() | NUM() | ID() | function_call;